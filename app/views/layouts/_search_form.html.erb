<%= form_tag('/search', local: true, method: :get, class: 'form-inline margin-top sign-up-form' ) do %>
  <%= text_field :q, placeholder: "Search 1000s of images", data: { behavior: "autocomplete" }, class: "form-control", id: "email" %>
  <%= button_tag "Search", class: "btn btn-primary", id: "submit", type: 'submit' %>
<% end %>